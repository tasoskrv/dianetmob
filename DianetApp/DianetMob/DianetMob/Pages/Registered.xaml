<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="DianetMob.Pages.Registered">
  <ContentPage.Content>
    <StackLayout VerticalOptions="Center">
      <Label Text="Please check your email to complete your registration." VerticalOptions="FillAndExpand" 
             HorizontalTextAlignment="Center" FontSize="Large" FontAttributes="Bold"/>
      <Button Text="Go to login" Clicked="OnClickLogin" Margin="0,10,0,0"/>
    </StackLayout>
  </ContentPage.Content>
</ContentPage>